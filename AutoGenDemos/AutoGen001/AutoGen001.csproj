<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<!-- This enables structural xml document support -->
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="AutoGen" Version="0.1.0" />
    <PackageReference Include="AutoGen.DotnetInteractive" Version="0.1.0" />
    <PackageReference Include="AutoGen.OpenAI" Version="0.1.0" />
    <PackageReference Include="AutoGen.SourceGenerator" Version="0.1.0" />
    <PackageReference Include="FluentAssertions" Version="7.0.0-alpha.4" />
  </ItemGroup>

</Project>
